{
    "name": "nitrado.js",
    "version": "0.1.15",
    "license": "GPL-3.0",
    "main": "index.js",
    "module": "index.mjs",
    "exports": {
        ".": {
            "require": "./index.js",
            "import": "./index.mjs"
        }
    },
    "author": "cainthebest <https://github.com/cainthebest> & Contributors",
    "description": "A type safe wrapper for the Nitrado API",
    "repository": {
        "type": "git",
        "url": "https://github.com/cainthebest/nitrado.js"
    },
    "bugs": {
        "url": "https://github.com/cainthebest/nitrado.js/issues"
    },
    "keywords": [
        "nitrapi",
        "Nitrado",
        "Nitrado API",
        "axios",
        "nodejs",
        "javascript",
        "typescript"
    ],
    "files": [
        "dist",
        "LICENSE",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "npm run format && npm run lint && npm run build:clean",
        "build:clean": "tsup-node",
        "lint:check": "eslint . --ext .ts",
        "lint": "npm run lint:check -- --fix",
        "format": "prettier --write **/**.{ts,json,md,yaml,yml}"
    },
    "dependencies": {
        "axios": "^0.27.2"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.23.0",
        "@typescript-eslint/parser": "^5.23.0",
        "eslint": "^8.15.0",
        "eslint-config-prettier": "^8.5.0",
        "prettier": "^2.6.2",
        "tsup": "^6.0.1",
        "typescript": "^4.6.4"
    }
}
